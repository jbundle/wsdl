<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
           xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
           targetNamespace="http://schemas.xmlsoap.org/wsdl/"
           elementFormDefault="qualified" > 

  <!-- Include all the schemas that you will be using -->
  <xs:include schemaLocation="http://schemas.xmlsoap.org/wsdl/2003-02-11.xsd"/>
  <!-- I need the soap spec too -->
  <xs:import namespace = "http://schemas.xmlsoap.org/wsdl/soap/" schemaLocation="http://schemas.xmlsoap.org/wsdl/soap/2003-02-11.xsd"/>

</xs:schema>
